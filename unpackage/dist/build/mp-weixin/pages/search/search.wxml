<view class="serach"><view class="search-songs"><view class="search-songs-title"><u--text vue-id="50cad900-1" bold="{{true}}" size="40" text="单曲" bind:__l="__l"></u--text></view><view class="search-songs-list"><block wx:for="{{childSongLists.music_list}}" wx:for-item="item" wx:for-index="index" wx:key="index"><view class="one-song"><view data-event-opts="{{[['tap',[['playOneMusic',[index]]]]]}}" class="song-info" bindtap="__e"><view class="song-cover"><u--image vue-id="{{'50cad900-2-'+index}}" radius="10" showLoading="{{true}}" src="{{item.music_cover}}" width="120" height="120" bind:__l="__l"></u--image></view><view class="song-name-singer"><u--text vue-id="{{'50cad900-3-'+index}}" color="{{childCurrentMusic==index?'#3c9cff':''}}" lines="1" size="34" text="{{item.music_name}}" bind:__l="__l"></u--text><u--text vue-id="{{'50cad900-4-'+index}}" color="{{childCurrentMusic==index?'#3c9cff':''}}" lines="1" size="28" text="{{item.music_singer}}" bind:__l="__l"></u--text></view></view><view class="song-button-group"><view class="play-button"><u-icon vue-id="{{'50cad900-5-'+index}}" size="50" name="download" data-event-opts="{{[['^click',[['downloadMusic',[index]]]]]}}" bind:click="__e" bind:__l="__l"></u-icon></view><u-icon vue-id="{{'50cad900-6-'+index}}" color="{{item.is_like?'red':''}}" size="50" name="{{item.is_like?'heart-fill':'heart'}}" data-event-opts="{{[['^click',[['collectOrCancelMusic',[index]]]]]}}" bind:click="__e" bind:__l="__l"></u-icon></view></view></block></view><block wx:if="{{$root.m0>8}}"><view class="search-songs-more"><u-icon vue-id="50cad900-7" size="28" color="black" labelSize="28" labelPos="left" label="{{'查看全部'+totalSongNumber+'首单曲'}}" name="arrow-right" data-event-opts="{{[['^click',[['viewAllSong']]]]}}" bind:click="__e" bind:__l="__l"></u-icon></view></block><block wx:if="{{$root.m1}}"><view class="warning-no-data"><u-icon vue-id="50cad900-8" size="40" labelSize="40" label="数据为空!" name="warning" bind:__l="__l"></u-icon></view></block></view><view class="search-playlists"><view class="search-playlist-title"><u--text vue-id="50cad900-9" bold="{{true}}" size="40" text="歌单" bind:__l="__l"></u--text></view><view class="search-playlist-list"><block wx:for="{{playlistlist.list}}" wx:for-item="item" wx:for-index="index" wx:key="index"><view data-event-opts="{{[['tap',[['viewPlaylist',[index]]]]]}}" class="one-playlist" bindtap="__e"><view class="playlist-cover"><u--image vue-id="{{'50cad900-10-'+index}}" radius="10" src="{{item.cover}}" width="120" height="120" bind:__l="__l"></u--image></view><view class="playlist-name-introduction"><u--text vue-id="{{'50cad900-11-'+index}}" lines="1" size="34" text="{{item.name}}" bind:__l="__l"></u--text><u--text vue-id="{{'50cad900-12-'+index}}" lines="1" size="26" text="{{item.introduction}}" bind:__l="__l"></u--text></view></view></block></view><block wx:if="{{$root.m2>8}}"><view class="search-playlist-more"><u-icon vue-id="50cad900-13" size="28" color="black" labelSize="28" labelPos="left" label="{{'查看全部'+totalPlaylistNumber+'个歌单'}}" name="arrow-right" data-event-opts="{{[['^click',[['viewAllPlaylist']]]]}}" bind:click="__e" bind:__l="__l"></u-icon></view></block><block wx:if="{{$root.m3}}"><view class="warning-no-data"><u-icon vue-id="50cad900-14" size="40" labelSize="40" label="数据为空!" name="warning" bind:__l="__l"></u-icon></view></block></view><u-loading-page vue-id="50cad900-15" iconSize="38" fontSize="38" loading="{{loadingStatus}}" bind:__l="__l"></u-loading-page></view>